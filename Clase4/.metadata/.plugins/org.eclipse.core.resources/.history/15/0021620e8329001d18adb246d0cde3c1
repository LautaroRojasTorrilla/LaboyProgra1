float ConvertirCaF(float numero)
{
    int resultado;

    resultado = (numero * 9 / 5 ) + 32;

    return resultado;
}

float ConvertirFaC(float numero)
{
    int resultado;

    resultado = (numero - 32) * 5 / 9;

    return resultado;
}
