#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);

	int numero;
	int* pNumero;

	pNumero = &numero;

	//le asigno 10 a mi variable numero
	numero = 10;

	*pNumero = 25;

	printf("%d\n", pNumero);

	return EXIT_SUCCESS;
}
