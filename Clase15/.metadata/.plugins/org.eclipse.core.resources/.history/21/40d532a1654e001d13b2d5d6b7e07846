#include <stdio.h>
#include <stdlib.h>

int OrdenarEnteros(int* numero1, int* numero2);

int main()
{
	printf("Hello World");

	int numero1 = 2;
	int numero1 = 5;

	OrdenarEnteros();


	return 0;
}

int OrdenarEnteros(int* numero1, int* numero2)
{
	int retorno = -1;
	int bufferNumero;

	if(numero1 =! NULL && numero2 != NULL && numero1 < numero2)
	{
		bufferNumero = *numero1;
		*numero1 = *numero2;
		*numero2 = bufferNumero;
	}

return retorno;
}
