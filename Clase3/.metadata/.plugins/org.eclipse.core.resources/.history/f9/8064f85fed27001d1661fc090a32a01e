#include <stdio.h>
#include <stdlib.h>

//prototipo o firma.
//tipo o clase que retorna - nombre de la función - parámetros de la función.

int calcularSuma(int numero1, int numero2);

int main(void) {
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}

int calcularSuma(int numero1, int numero2)
{
	int resultado;

	resultado = numero1 + numero2;

	return resultado;
}



