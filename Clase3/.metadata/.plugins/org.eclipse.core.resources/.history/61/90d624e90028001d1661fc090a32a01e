#include <stdio.h>
#include <stdlib.h>

void sumar1(void);
void sumar2(int, int);

int main(void) {
	setbuf(stdout, NULL);

	int primerNumero = 9;
	int segundoNumero = 11;

	//sumar1();

	sumar2(primerNumero, segundoNumero);
	sumar2(15,25);


	return EXIT_SUCCESS;
}

void sumar1(void)
{
	int numero1;
	int numero2;

	int suma;

	printf("Ingrese un numero: ");
	scanf("%d",&numero1);
	printf("Ingrese otr numero: ");
	scanf("%d",&numero2);

	suma = numero1 + numero2;

	printf("La suma es: %d\n",suma);

}

void sumar2(int numero1, int numero2)
{
	int suma;

	suma = numero1 + numero2;

	printf("La suma es: %d\n",suma);
}
