#include <stdio.h>
#include <stdlib.h>

void sumar1(void);

int main(void) {
	setbuf(stdout, NULL);

	return EXIT_SUCCESS;
}

void sumar1(void)
{
	int numero1;
	int numero2;

	int suma;

	printf("Ingrese un numero: ");
	scanf("%d",&numero1);
	printf("Ingrese otr numero: ");
	scanf("%d",&numero2);

	suma = numero1 + numero2;

	printf("La suma es: %d",suma);

}
