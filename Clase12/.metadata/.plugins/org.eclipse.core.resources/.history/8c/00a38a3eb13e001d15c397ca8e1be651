#include <stdio.h>
#include <stdlib.h>

typedef struct{
	int dia;
	int mes;
	int anio;
}eFecha;

typedef struct{
	char ApeNom[51];
	char dni[9];//porque no se le hace ningun calculo aritmetico o matematico
	eFecha fechaNac;
}ePersona;

typedef struct{
	int id;
	char descripcion[31];
}ePais;

typedef struct{

}eProvincia;


int main(void) {
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
