#include <stdio.h>
#include <stdlib.h>

typedef struct{
	int dia;
	int mes;
	int anio;
}eFecha;

typedef struct{
	char ApeNom[51];
	char dni[9];//porque no se le hace ningun calculo aritmetico o matematico
	eFecha fechaNac;
	char sexo;
	int nacionalidad;
}ePersona;

typedef struct{
	int id;
	char descripcion[31];
}ePais;

typedef struct{
	int id;
	char descripcion[31];
}eProvincia;
//1. BUENOS AIRES
//2. SANTA FE
//3. MISIONES

typedef struct{
	int id;
	char descripcion[31];
}eLocalidad;
//1. AVELLANEDA
//2. QUILMES
//3. LANUS


int main(void) {
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
