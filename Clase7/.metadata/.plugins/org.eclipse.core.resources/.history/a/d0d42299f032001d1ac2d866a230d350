#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
	setbuf(stdout,NULL);

	char cadena[30];

	printf("Ingrese texto: ");

	scanf("%s", cadena);

	printf("%s\n", cadena);
	puts(cadena);

	return EXIT_SUCCESS;
}
