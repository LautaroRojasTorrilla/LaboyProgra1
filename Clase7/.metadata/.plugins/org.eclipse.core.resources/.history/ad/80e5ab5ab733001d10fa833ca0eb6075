#define CANTIDAD 100

int main(void) {
	setbuf(stdout,NULL);

	int lejago[CANTIDAD];
	float salario[CANTIDAD];
	char sexo [CANTIDAD];
	char nombre[CANTIDAD][31];

	return EXIT_SUCCESS;
}
