#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {

	char cadena[30];

	printf("Ingrese su nombre");

	scanf("%s", cadena);

	printf("%s", cadena);

	return EXIT_SUCCESS;
}
